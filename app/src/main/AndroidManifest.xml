<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ntacey.runwatch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ntacey.runwatch.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ntacey.runwatch.activity.RouteListActivity"></activity>
        <activity android:name="com.ntacey.runwatch.activity.RouteMainActivity"></activity>
        <activity android:name="com.ntacey.runwatch.activity.RouteWatchActivity"></activity>
        <activity android:name="com.ntacey.runwatch.activity.NameRouteActivity"></activity>
    </application>

</manifest>